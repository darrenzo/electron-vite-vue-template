{
    "name": "reolink",
    "description": "Reolink application",
    "author": "Shenzhen Reolink Technology Co., Ltd.",
    "private": true,
    "version": "8.10.2",
    "main": "./dist/electron/main/main.js",
    "encryptionLevel": 0,
    "scripts": {
        "dev": "esno .electron-vite/dev-runner.ts",
        "build": "cross-env BUILD_TARGET=clean esno .electron-vite/build.ts -m prod && electron-builder -c build.json",
        "build:win32": "cross-env BUILD_TARGET=clean esno .electron-vite/build.ts  && electron-builder -c build.json --win  --ia32",
        "build:win64": "cross-env BUILD_TARGET=clean esno .electron-vite/build.ts  && electron-builder -c build.json --win  --x64",
        "build:mac": "cross-env BUILD_TARGET=clean esno .electron-vite/build.ts  && electron-builder -c build.json --mac",
        "build:dir": "cross-env BUILD_TARGET=clean esno .electron-vite/build.ts  && electron-builder -c build.json --dir",
        "build:clean": "cross-env BUILD_TARGET=onlyClean esno .electron-vite/build.ts",
        "build:web": "cross-env BUILD_TARGET=web esno .electron-vite/build.ts",
        "dep:upgrade": "yarn upgrade-interactive --latest",
        "typecheck": "vue-tsc --noEmit",
        "commit": "cz",
        "lint": "yarn lint:fix && yarn lint:format && yarn lint:style",
        "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json,vue}\" ",
        "lint:style": "stylelint ./**/*.{css,less,vue,html} --fix",
        "postinstall": "electron-builder install-app-deps",
        "prepare": "husky install"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/@commitlint/cz-commitlint"
        }
    },
    "dependencies": {
        "@reolink/cli.bcsdk": "1.2243.7-helper",
        "axios": "^1.4.0",
        "electron-log": "^4.4.8",
        "electron-updater": "^5.3.0",
        "semver": "^7.5.0",
        "uuid": "^9.0.0",
        "vue-i18n": "^9.2.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.6",
        "@commitlint/config-conventional": "^17.6.6",
        "@commitlint/cz-commitlint": "^17.5.0",
        "@rollup/plugin-alias": "^4.0.3",
        "@rollup/plugin-commonjs": "^24.1.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-replace": "^5.0.2",
        "@rushstack/eslint-patch": "^1.3.2",
        "@tsconfig/node18": "^2.0.1",
        "@types/fs-extra": "^11.0.1",
        "@types/minimist": "^1.2.2",
        "@types/node": "^18.16.2",
        "@vitejs/plugin-vue": "^4.2.1",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/tsconfig": "^0.4.0",
        "adm-zip": "^0.5.10",
        "base64url": "^3.0.1",
        "cfonts": "^3.1.1",
        "chalk": "5.2.0",
        "commitizen": "^4.3.0",
        "cross-env": "^7.0.3",
        "del": "^7.0.0",
        "dotenv": "^16.0.3",
        "electron": "20.0.2",
        "electron-builder": "^23.6.0",
        "electron-devtools-vendor": "^1.2.0",
        "element-plus": "^2.3.4",
        "eslint": "^8.43.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-vue": "^9.15.0",
        "esno": "^0.16.3",
        "extract-zip": "^2.0.1",
        "fs-extra": "^11.1.1",
        "husky": "^8.0.3",
        "javascript-obfuscator": "^4.0.2",
        "less": "^4.1.3",
        "lint-staged": "^13.2.3",
        "listr2": "^5.0.7",
        "minimist": "^1.2.8",
        "pinia": "^2.1.4",
        "portfinder": "^1.0.32",
        "postcss": "^8.4.24",
        "postcss-html": "^1.5.0",
        "postcss-less": "^6.0.0",
        "prettier": "^2.8.8",
        "rollup-plugin-esbuild": "^5.0.0",
        "rollup-plugin-obfuscator": "^1.0.3",
        "stylelint": "^15.9.0",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-recommended-less": "^1.0.4",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard": "^33.0.0",
        "stylelint-less": "^1.0.7",
        "stylelint-order": "^6.0.3",
        "typescript": "5.0.4",
        "vite": "^4.1.4",
        "vue": "3.2.47",
        "vue-router": "^4.1.6",
        "vue-tsc": "^1.8.1"
    },
    "keywords": [
        "vite",
        "electron",
        "vue3",
        "rollup"
    ]
}
